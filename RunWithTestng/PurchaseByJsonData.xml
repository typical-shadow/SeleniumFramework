<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Ecommerce Application">

	<groups>
		<run>
			<include name="Purchase"></include>
		</run>
	</groups>

	<test thread-count="5" name="Complete Place Order Flow">
		<classes>
			<class name="rahualshetty.tests.PlaceOrderFlow" />
		</classes>
	</test> <!-- Test -->

	<test thread-count="5" name="Login Error validation Flow">
		<classes>
			<class name="rahualshetty.tests.ErrorValidation" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->